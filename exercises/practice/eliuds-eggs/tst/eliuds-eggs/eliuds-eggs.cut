TestCase "0 eggs"
	MOVE 0 TO WS-INPUT
	PERFORM EGG-COUNT
	EXPECT WS-RESULT = 0

TestCase "1 egg"
	MOVE 16 TO WS-INPUT
	PERFORM EGG-COUNT
	EXPECT WS-RESULT = 1

TestCase "4 eggs"
	MOVE 89 TO WS-INPUT
	PERFORM EGG-COUNT
	EXPECT WS-RESULT = 4

TestCase "13 eggs"
	MOVE 2000000000 TO WS-INPUT
	PERFORM EGG-COUNT
	EXPECT WS-RESULT = 13

