TestCase "Roster is empty when no student is added"
	PERFORM INIT-ROSTER
	EXPECT ST-NAME(1) = SPACES
	EXPECT ST-GRADE(1) = 0
	EXPECT ST-NAME(2) = SPACES
	EXPECT ST-GRADE(2) = 0
	EXPECT ST-NAME(3) = SPACES
	EXPECT ST-GRADE(3) = 0


TestCase "Student is added to the roster"
	PERFORM INIT-ROSTER
	MOVE "Aimee" TO WS-STUDENTNAME
	MOVE 2 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT
	EXPECT ST-NAME(1) = "Aimee"
	EXPECT ST-GRADE(1) = 2
	EXPECT ST-NAME(2) = SPACES
	EXPECT ST-GRADE(2) = 0
	EXPECT ST-NAME(3) = SPACES
	EXPECT ST-GRADE(3) = 0


TestCase "Multiple students in the same grade are added to the roster"
	PERFORM INIT-ROSTER
	MOVE "Blair" TO WS-STUDENTNAME
	MOVE 2 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT
	MOVE "James" TO WS-STUDENTNAME
	MOVE 2 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT	
	MOVE "Paul" TO WS-STUDENTNAME
	MOVE 2 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT
	EXPECT ST-NAME(1) = "Blair"
	EXPECT ST-GRADE(1) = 2
	EXPECT ST-NAME(2) = "James"
	EXPECT ST-GRADE(2) = 2
	EXPECT ST-NAME(3) = "Paul"
	EXPECT ST-GRADE(3) = 2


TestCase "Students in multiple grades are added to the roster"
	PERFORM INIT-ROSTER
	MOVE "Chelsea" TO WS-STUDENTNAME
	MOVE 3 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT
	MOVE "Logan" TO WS-STUDENTNAME
	MOVE 7 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT	
	EXPECT ST-NAME(1) = "Chelsea"
	EXPECT ST-GRADE(1) = 3
	EXPECT ST-NAME(2) = "Logan"
	EXPECT ST-GRADE(2) = 7
	EXPECT ST-NAME(3) = SPACES
	EXPECT ST-GRADE(3) = 0
	EXPECT ST-NAME(4) = SPACES
	EXPECT ST-GRADE(4) = 0


TestCase "A student can't be in two different grades"
	PERFORM INIT-ROSTER
	MOVE "Aimee" TO WS-STUDENTNAME
	MOVE 2 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT
	MOVE "Aimee" TO WS-STUDENTNAME
	MOVE 1 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT	
	EXPECT ST-NAME(1) = "Aimee"
	EXPECT ST-GRADE(1) = 2
	EXPECT ST-NAME(2) = SPACES
	EXPECT ST-GRADE(2) = 0
	EXPECT ST-NAME(3) = SPACES
	EXPECT ST-GRADE(3) = 0
	EXPECT ST-NAME(4) = SPACES
	EXPECT ST-GRADE(4) = 0


