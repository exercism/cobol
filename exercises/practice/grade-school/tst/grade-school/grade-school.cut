	EXPECT ST-NAME(3) = "Paul"
	EXPECT ST-GRADE(3) = 2
	EXPECT ST-NAME(4) = SPACES
	EXPECT ST-GRADE(4) = 0


TestCase "Students are sorted by grades in the roster"
	PERFORM INIT-ROSTER
	MOVE "Jim" TO WS-STUDENTNAME
	MOVE 3 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT
	MOVE "Peter" TO WS-STUDENTNAME
	MOVE 2 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT	
	MOVE "Anna" TO WS-STUDENTNAME
	MOVE 1 TO WS-STUDENTGRADE
	PERFORM ADD-STUDENT	
	EXPECT ST-NAME(1) = "Anna"
	EXPECT ST-GRADE(1) = 1
	EXPECT ST-NAME(2) = "Peter"
	EXPECT ST-GRADE(2) = 2
	EXPECT ST-NAME(3) = "Jim"
	EXPECT ST-GRADE(3) = 3
	EXPECT ST-NAME(4) = SPACES
	EXPECT ST-GRADE(4) = 0


